<!DOCTYPE html>
<html lang="es">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no" />
  <title>@ViewData["Title"] </title>
  <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
  <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
  <link rel="stylesheet" href="~/css/yety.css" />
  <link rel="icon" type="img" href="~/img/logo.ico">
  <!-- Compiled and minified Bootstrap CSS -->
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
</head>

<body>
  <header>
    <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-dark bg-primary border-bottom box-shadow mb-6">
      <div class="container">
        <!-- Image and text -->
        <a class="navbar-brand" href="index">
          <img src="~/img/huella.svg" width="30" height="30" class="d-inline-block align-top" alt="">
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse"
          aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
          <!-- -->
          <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
            <li class="nav-item active">
              <a class="nav-link" href="index">&nbsp Inicio </a>
            </li>
            <li class="nav-item active">
              <a class="nav-link" href="nosotros">&nbsp &nbsp Quiénes somos &nbsp</a>
              <!--<span class="sr-only">(current)</span> -->
            </li>
            <li class="nav-item dropdown nav-item active">
              <a class="nav-link dropdown-toggle" href="#" id="dropdownId" data-bs-toggle="dropdown"
                aria-haspopup="true" aria-expanded="false">Servicios</a>
              <div class="dropdown-menu" aria-labelledby="dropdownId">
                <a class="dropdown-item" href="#">Consulta general</a>

                <a class="dropdown-item" href="#">Consulta especializada</a>

                <a class="dropdown-item" href="#">Urgencias veterinarias</a>

                <a class="dropdown-item" href="#">Medecina preventiva</a>
              </div>
            </li>
            <li class="nav-item dropdown nav-item active">
              <a class="nav-link dropdown-toggle" href="#" id="dropdownId" data-bs-toggle="dropdown"
                aria-haspopup="true" aria-expanded="false">Complementarios</a>
              <div class="dropdown-menu" aria-labelledby="dropdownId">
                <a class="dropdown-item" href="#">Nutrición</a>

                <a class="dropdown-item" href="#">Farmacia</a>

                <a class="dropdown-item" href="#">Programa de bienestar animal</a>
              </div>
            </li>

            <li class="nav-item active">
              <a class="nav-link" href="noticias">&nbsp &nbsp Notas de interés </a>
            </li>
            <li class="nav-item active">
              <a class="nav-link" href="contacto">&nbsp &nbsp Contacto &nbsp &nbsp</a>
            </li>

            <li class="nav-item active">
              <a class="nav-link" href="modulos">Modulos &nbsp &nbsp &nbsp &nbsp &nbsp</a>
            </li>

            <div class="d-flex flex-row">
              <li class="nav justify-content-end">
                <a class="btn btn-lg btn-warning" href="iniciar-sesion" role="button">Iniciar sesión</a>
              </li>
            </div>
          </ul>
        </div>
      </div>
    </nav>
  </header>
  <div class="row">
    <div class="col-lg-12">
      <main role="main" class="pb-3">
        @RenderBody()
      </main>
    </div>
  </div>

  <footer class="footer text-muted">

    <div class="copy">
      <ul class="copyright" class="privacy">
        <li><a href="#">© Copyright Veterinaria Mundo Animal - 2022</a></li>
        <li><a href="Privacy">Condiciones del servicio y política de tratamiento de datos</a></li>
      </ul>
    </div>
  </footer>
  <script src="~/lib/jquery/dist/jquery.min.js"></script>
  <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
  <script src="~/js/site.js" asp-append-version="true"></script>
  <script src="~/js/jquery-1.9.1.min.js" asp-append-version="true"></script>

  @RenderSection("Scripts", required: false)
</body>
</html>
